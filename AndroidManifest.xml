    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="21" />

    <application
        android:name="com.pczhu.jiugongge.BaseApplication"
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.pczhu.jiugongge.ui.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.pczhu.jiugongge.ui.LoginActivity"></activity>
        <activity android:name="com.pczhu.jiugongge.ui.SettingActivity"></activity>
        <activity android:name="com.pczhu.jiugongge.ui.LockActivity"></activity>
    </application>

</manifest>
